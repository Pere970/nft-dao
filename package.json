  "main": "index.js",
  "scripts": {
    "deploy:contracts:localhost": "npx hardhat deploy --network localhost --tags $npm_config_tags",
    "deploy:contracts:hardhat": "npx hardhat deploy --network hardhat --tags $npm_config_tags",
    "deploy:contracts:rinkeby": "npx hardhat deploy --network rinkeby --tags $npm_config_tags",
    "compile": "hardhat compile",
    "format": "npx prettier --write \"**/*.js\" \"**/*.sol\"",
    "lint": "eslint . --ext .js && solhint 'contracts/**/*.sol'",
    "test": "hardhat test",
    "coverage": "hardhat coverage"
  },
